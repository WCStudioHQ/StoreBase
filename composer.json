{
    "name": "wcstudio/storebase",
    "type": "wordpress-theme",
    "description": "Built for online stores, this WooCommerce theme is the ultimate choice for creating a beautiful and functional e-commerce site. Featuring customizable product pages, advanced filtering, and seamless checkout options, itâ€™s designed to provide an excellent shopping experience on any device.",
    "keywords": [
        "WordPress",
        "Themes"
    ],
    "homepage": "https://github.com/Automattic/_s",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Contributors",
            "homepage": "https://github.com/Automattic/_s/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "php-parallel-lint/php-parallel-lint": "^1.2.0",
        "wp-cli/i18n-command": "^2.2.5",
        "wp-coding-standards/wpcs": "^3.0",
        "squizlabs/php_codesniffer": "^3.11",
        "phpcompatibility/phpcompatibility-wp": "^2.1"
    },
    "scripts": {
        "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "fix:wpcsbf": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf --standard=WordPress .",
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
        "make-pot": "wp i18n make-pot . languages/storebase.pot"
    },
    "support": {
        "issues": "https://github.com/Automattic/_s/issues",
        "source": "https://github.com/Automattic/_s"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
